{
    "output_dir": "lora_models/xander_sd15",
    "sd_model_version": "sd15",
    "lora_training_urls": "https://storage.googleapis.com/public-assets-xander/A_workbox/lora_training_sets/xander.zip",
    "concept_mode": "face",
    "seed": 1,
    "resolution": 640,
    "train_batch_size": 4,
    "n_sample_imgs": 6,
    "max_train_steps": 600,
    "token_warmup_steps": 20,
    "checkpointing_steps": 100,
    "gradient_accumulation_steps": 1,

    "sample_imgs_lora_scale": 0.7,
    "n_tokens": 2,
    "ti_lr": 0.001,
    "remove_ti_token_from_prompts": false,

    "text_encoder_lora_optimizer": "adamw",
    "text_encoder_lora_lr": 1.0e-4,
    "text_encoder_lora_weight_decay": 1e-5,
    "text_encoder_lora_rank": 12,

    "unet_lr": 0.0005,
    "lora_alpha_multiplier": 1.0,
    "lora_rank": 16,
    "use_dora": false,
    "caption_model": "gpt4-v",
    "debug": true
}